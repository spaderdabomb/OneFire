.level-container-even {
    flex-grow: 0;
    justify-content: center;
    align-items: flex-start;
}

.level-container-odd {
    flex-direction: column-reverse;
    align-items: flex-start;
}

.level-node-container-right {
    flex-grow: 0;
    align-items: flex-start;
    align-self: flex-start;
    flex-direction: row;
}

.level-node-container-standard {
    flex-grow: 0;
    align-items: center;
    align-self: auto;
    align-content: auto;
}

.level-node-standard {
    flex-grow: 0;
    width: 4px;
    height: 75px;
    background-color: rgb(58, 37, 12);
    left: 12px;
    top: 2px;
}

.level-node {
    flex-grow: 0;
    background-color: rgb(89, 58, 20);
    rotate: 45deg;
    width: 28px;
    height: 28px;
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
    border-left-color: rgb(58, 37, 12);
    border-right-color: rgb(58, 37, 12);
    border-top-color: rgb(58, 37, 12);
    border-bottom-color: rgb(58, 37, 12);
    -unity-text-align: upper-left;
    justify-content: center;
    align-self: center;
    align-items: center;
    left: auto;
    margin-top: 2px;
    margin-right: 0;
    margin-bottom: 1px;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    bottom: auto;
    top: auto;
}

