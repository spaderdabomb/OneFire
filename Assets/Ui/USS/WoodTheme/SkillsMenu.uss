.level-path-right {
    flex-grow: 0;
    width: 75px;
    height: 4px;
    background-color: rgb(58, 37, 12);
    left: 2px;
    top: 14px;
    margin-right: 4px;
    margin-left: 3px;
}

.level-path-standard {
    flex-grow: 0;
    width: 4px;
    height: 75px;
    background-color: rgb(58, 37, 12);
    left: 12px;
    top: 2px;
    bottom: auto;
    margin-bottom: 2px;
}

.level-node-container-reversed {
    flex-grow: 0;
    align-items: center;
    align-self: auto;
    align-content: auto;
    flex-direction: column-reverse;
}

.level-node-container-standard {
    flex-grow: 0;
    align-items: center;
    align-self: auto;
    align-content: auto;
}

